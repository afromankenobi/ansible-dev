# Elixir default packages - installed automatically by asdf when installing Elixir versions
# Base packages (always installed)
{% for pkg in default_elixir_base %}
{{ pkg }}
{% endfor %}

# Optional packages (from playbook variables)
{% if default_elixir_optional is defined %}
{% for pkg in default_elixir_optional %}
{{ pkg }}
{% endfor %}
{% endif %}
